{
  "name": "@kurocado-studio/formkit-store",
  "version": "0.0.0-semantically-released",
  "private": true,
  "type": "module",
  "main": "./dist/exports.js",
  "module": "./dist/exports.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/exports.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/exports.js"
    }
  },
  "scripts": {
    "domain:build": "rm -rf dist && tsup",
    "dev": "rm -rf dist && tsup --watch --dts",
    "commitlint": "pnpm exec commitlint --edit .git/COMMIT_EDITMSG",
    "clean": "rm -rf node_modules pnpm.lock dist",
    "eslint-check": "eslint --max-warnings=0  --debug",
    "eslint-fix": "eslint --max-warnings=0  --fix --no-warn-ignored",
    "lint": "eslint --max-warnings=0  --no-warn-ignored",
    "prepare": "husky install",
    "prettier-check": "prettier --check ",
    "prettier-fix": "prettier --check  --write",
    "semantic-release": "semantic-release",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit --pretty",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "setup": "pnpm clean && pnpm i && pnpm build"
  },
  "dependencies": {
    "@kurocado-studio/formkit-ui-models": "1.0.0-develop.2",
    "lodash-es": "^4.17.21",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@internal/config": "workspace:*",
    "@kurocado-studio/qa": "2.0.0-alpha.2",
    "@vitest/coverage-v8": "^3.0.5",
    "tsup": "^8.5.1",
    "typescript": "5.8.3",
    "vitest": "^3.0.5"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  }
}
