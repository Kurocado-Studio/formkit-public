{
  "name": "@kurocado-studio/formkit-public-monorepo",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "build": "turbo run build",
    "deploy": "pnpm deploy:dev && pnpm deploy:prod",
    "clean": "turbo run clean && rm -rf .turbo node_modules pnpm-lock.yaml",
    "commitlint": "pnpm exec commitlint --edit .git/COMMIT_EDITMSG",
    "dev": "turbo run dev --concurrency=20",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepare": "husky install",
    "prettier:check": "prettier --check . --ignore-path .prettierignore && turbo run prettier:check",
    "prettier:fix": "prettier --check --write . --ignore-path .prettierignore && turbo run prettier:fix",
    "preview-storybook": "turbo run preview-storybook",
    "semantic-release": "pnpm -r --workspace-concurrency=1 exec -- npx --no-install semantic-release -e semantic-release-monorepo",
    "setup": "pnpm clean && pnpm i && turbo run setup",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@internal/config": "workspace:*",
    "@kurocado-studio/qa": "1.5.1-develop.1",
    "@kurocado-studio/styleguide": "2.6.0-develop.17",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.10",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.21",
    "commitlint": "^19.8.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.0.0",
    "eslint": "9.31",
    "husky": "^9.1.7",
    "import": "^0.0.6",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.3",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semantic-release": "^25.0.2",
    "semantic-release-monorepo": "^8.0.2",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.5.1",
    "turbo": "^2.5.6",
    "typescript": "5.1.6",
    "vite": "5.2.8",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "prettier --write",
      "turbo run lint:fix --",
      "turbo run lint --"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "overrides": {
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.10",
    "vite": "5.2.8",
    "typescript": "5.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.12",
      "rollup": "4.52.0"
    }
  }
}
