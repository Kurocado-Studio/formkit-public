{
  "name": "@internal/config",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "private": true,
  "keywords": [],
  "author": "@kurocado-studio",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tsconfig.web": "./dist/tsconfig.web.json",
    "./qa/web/setup": "./dist/setup.web.js"
  },
  "files": ["dist"],
  "scripts": {
    "internal:build": "pnpm install && rm -rf dist && tsup --dts",
    "internal:dev": "rm -rf dist && tsup --watch --dts",
    "internal:clean": "rm -rf .turbo node_modules dist",
    "lint": "eslint --max-warnings=0 . --no-warn-ignored",
    "lint:fix": "eslint --max-warnings=0 . --fix --no-warn-ignored",
    "prettier:fix": "prettier --write . --ignore-path ../../.prettierignore",
    "prettier:check": "prettier --check . --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit --pretty"
  }
}
