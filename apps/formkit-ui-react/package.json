{
  "name": "@kurocado-studio/formkit-ui-react",
  "version": "0.0.0-semantically-released",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "commitlint": "pnpm exec commitlint --edit .git/COMMIT_EDITMSG",
    "clean": "rm -rf node_modules pnpm.lock dist",
    "dev": "cross-env VITE_NODE_ENV=local vite",
    "eslint-check": "eslint --max-warnings=0 . --debug",
    "eslint-fix": "eslint --max-warnings=0 . --fix --no-warn-ignored",
    "lint": "eslint --max-warnings=0 . --no-warn-ignored",
    "prepare": "husky install",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --check . --write",
    "semantic-release": "semantic-release",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit --pretty",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "setup": "pnpm clean && pnpm i && pnpm build",
    "use:local-ui": "pnpm remove @kurocado-studio/ui && pnpm add @kurocado-studio/ui@file:../design-system/packages/ui",
    "use:remote-ui": "pnpm remove @kurocado-studio/ui && pnpm add @kurocado-studio/ui@latest",
    "use:remote-html-forms": "pnpm remove @kurocado-studio/html-forms && pnpm add @kurocado-studio/html-forms@file:../platform/teams/dev-experience/packages/html-forms",
    "use:local-html-forms": "pnpm remove @kurocado-studio/html-forms && pnpm add @kurocado-studio/html-forms@file:../platform/teams/dev-experience/packages/html-forms",
    "use:build-local-html-package": "cd ../platform/teams/dev-experience/packages/html-forms && pnpm build",
    "use:sync-local-html-package": "pnpm clean && pnpm add @kurocado-studio/html-forms@file:../platform/packages/dev-experience/html-forms && pnpm i",
    "ui:local:lib": "pnpm add @kurocado-studio/ui-react-research-and-development@file:../../../design-system/packages/ui",
    "ui:canary:lib": "pnpm add @kurocado-studio/ui-react-research-and-development@canary",
    "ui:published:lib": "pnpm add @kurocado-studio/ui-react-research-and-development@latest",
    "ui:local:sync": "cd ../design-system-research-and-development/packages/ui-research-and-development && pnpm run build && cd - && pnpm install @kurocado-studio/ui-react-research-and-development@file:../design-system-research-and-development/packages/ui-research-and-development",
    "ui:sync:dev": "pnpm clean && pnpm ui:local:sync && pnpm use:build-local-html-package && pnpm use:sync-local-html-package && pnpm i && pnpm dev"
  },
  "lint-staged": {
    "*": ["pnpm run prettier-fix"],
    "**/*.{ts,tsx}": [
      "pnpm run prettier-fix",
      "pnpm run eslint-fix",
      "pnpm run lint"
    ]
  },
  "dependencies": {
    "@conform-to/dom": "^1.10.1",
    "@conform-to/react": "^1.10.1",
    "@conform-to/zod": "^1.9.0",
    "@kurocado-studio/axios-react": "^1.0.0",
    "@kurocado-studio/formkit-store": "workspace:*",
    "@kurocado-studio/formkit-ui-models": "1.0.0-develop.2",
    "@kurocado-studio/formkit-ui-react-renderer": "1.0.0-develop.3",
    "@kurocado-studio/react-design-system": "1.0.0-develop.24",
    "@kurocado-studio/react-utils": "1.0.0-develop.1",
    "@kurocado-studio/ui-react-research-and-development": "1.0.0-develop.17",
    "@react-aria/utils": "^3.30.1",
    "axios": "^1.12.0",
    "framer-motion": "^12.6.2",
    "isbot": "^5.1.25",
    "lodash-es": "^4.17.21",
    "react": "19.0.0",
    "react-aria": "^3.43.2",
    "react-dom": "19.0.0",
    "react18-json-view": "^0.2.9",
    "tailwind-merge": "^3.1.0",
    "zod": "^3.23.0",
    "zustand": "^5.0.8"
  },
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  },
  "resolutions": {
    "esbuild": "0.25.12",
    "rollup": "4.52.0"
  },
  "eslintConfig": {
    "extends": ["plugin:storybook/recommended"]
  }
}
